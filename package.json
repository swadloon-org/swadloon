{
  "name": "newrade",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "os": [
    "!win32"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "yarn lerna:build --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "build:postinstall": "yarn lerna:build:postinstall",
    "changelog": "lerna version --conventional-commits --no-push",
    "clean": "yarn lerna:clean",
    "cli": "nr",
    "commit": "lint-staged && git-cz",
    "deduplicate": "npx yarn-deduplicate yarn.lock && yarn",
    "depcheck": "yarn lerna:depcheck --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "dev": "yarn clean && yarn preconstruct dev",
    "format:changelog": "prettier --write packages/**/CHANGELOG.md --loglevel silent && git add packages/**/CHANGELOG.md",
    "lerna:build": "lerna run build",
    "lerna:build:postinstall": "lerna run build:postinstall --parallel",
    "lerna:clean": "lerna run clean",
    "lerna:depcheck": "lerna exec 'nr depcheck' --concurrency=1",
    "lerna:ls-lint": "lerna exec --concurrency=1 ls-lint",
    "lerna:start": "lerna run start --parallel",
    "lerna:test": "lerna run test",
    "lerna:test:e2e": "lerna run --concurrency=1 test:e2e",
    "lerna:typecheck": "lerna run typecheck --concurrency=4 --loglevel info",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "ls-lint": "ls-lint",
    "postinstall": "patch-package && yarn preconstruct:dev && yarn build:postinstall",
    "preconstruct:dev": "preconstruct dev",
    "push": "git push --follow-tags",
    "release": ". ./.env && GH_TOKEN=$GH_TOKEN lerna version --conventional-commits --create-release github",
    "reset": "rm -rf ./node_modules packages/**/node_modules && yarn",
    "start": "yarn lerna:start --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "test": "yarn lerna:test --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "test:e2e": "yarn lerna:test:e2e --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "test:ui": "yarn lerna:test --scope '{@newrade/core-design-system,@newrade/core-react-ui,@newrade/core-graphql-ui}'",
    "typecheck": "yarn lerna:typecheck --scope @newrade/core-* --ignore @newrade/core-graphql-ui",
    "yo": "nr-core-yo-generator"
  },
  "preconstruct": {
    "packages": [
      "packages/core-contentful-lib",
      "packages/core-design-system",
      "packages/core-common",
      "packages/core-utils",
      "packages/core-react-ui",
      "packages/core-gatsby-config",
      "packages/core-webpack-config",
      "packages/core-website-api",
      "packages/core-figma-extractor",
      "packages/core-jest-config"
    ]
  },
  "resolutions": {
