{
  "name": "@newrade/core-gatsby-ui",
  "version": "2.3.0",
  "description": "Reusable UI components for Gatsby sites",
  "license": "MIT",
  "main": "",
  "files": [
    "/src"
  ],
  "scripts": {
    "build": "yarn clean && yarn typecheck",
    "clean": "yarn clean:tsc && yarn clean:dist",
    "clean:dist": "rimraf -rf dist/** lib/**",
    "clean:tsc": "rimraf -f ./**/*.tsbuildinfo *.tsbuildinfo",
    "test": "nr jest",
    "test:watch": "env-cmd nr jest-watch",
    "typecheck": "tsc --noEmit --composite false --incremental false"
  },
  "sideEffects": [
    "*.css",
    "*.css.ts",
    "*.css.js",
    "*.treat.ts",
    "*.treat.js"
  ],
  "dependencies": {
    "@capsizecss/core": "^3.0.0",
    "@capsizecss/metrics": "^0.1.0",
    "@capsizecss/vanilla-extract": "^1.0.0",
    "@mdx-js/react": "^1.6.22",
    "@newrade/core-common": "^1.4.2",
    "@newrade/core-design-system": "^1.14.2",
    "@newrade/core-gatsb-config": "^3.1.0",
    "@newrade/core-jest-config": "^2.0.2",
    "@newrade/core-react-ui": "^2.3.0",
    "@newrade/core-types": "*",
    "@newrade/core-website-api": "^1.4.0",
    "@reach/router": "^1",
    "@react-google-maps/api": "^2.1.1",
    "@vanilla-extract/css": "^1.6.3",
    "@vanilla-extract/recipes": "^0.2.1",
    "case": "^1.6.3",
    "csstype": "^3",
    "gatsby": "^3.14.2",
    "gatsby-background-image": "^1",
    "gatsby-image": "^3.11.0",
    "i18next": "^19",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.31.0",
    "react-schemaorg": "^1.3.1",
    "react-treat": "^2",
    "schema-dts": "^0.8.2",
    "treat": "^2"
  }
}
