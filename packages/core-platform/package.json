{
  "name": "@newrade/core-website-platform",
  "version": "1.0.0",
  "private": true,
  "files": [
    "/dist",
    "/models"
  ],
  "scripts": {
    "prebuild": "prisma generate",
    "build": "next build",
    "clean": "rimraf -rf ./node_modules/.cache ./.next",
    "dev": "yarn preconstruct dev",
    "export": "next build && next export",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "lint": "next lint",
    "prisma:migrate": "prisma migrate deploy",
    "serve": "next start",
    "prestart": "prisma generate",
    "start": "env-cmd -x next dev -p \\$APP_PORT",
    "test": "nr jest",
    "test:watch": "nr jest-watch",
    "typecheck": "tsc --noEmit --composite false --incremental false"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@newrade/core-react-ui-test-component": "*",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.8.1",
    "@types/js-cookie": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "blurhash": "^1.1.4",
    "clsx": "^1.1.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "fs": "^0.0.1-security",
    "gray-matter": "^4.0.3",
    "js-cookie": "^3.0.1",
    "next": "^12",
    "next-auth": "^4.1.2",
    "next-mdx-remote": "^3.0.8",
    "next-plausible": "^3.1.4",
    "plaiceholder": "^2.2.0",
    "postcss": "^8.4.5",
    "prisma": "^3.8.1",
    "remark": "^14.0.2",
    "remark-mdx": "^2",
    "sharp": "^0.29.3",
    "sitemap": "^7.1.0",
    "swr": "^1.1.2",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2"
  }
}
